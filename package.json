  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!/node_modules/",
      "!src/api/*",
      "!src/(index|setupTests|registerServiceWorker).js",
      "!src/store/(configureStore|rootReducer|rootSaga).js",
      "!src/store/*/actions/types.js",
      "!src/emerald-ui/**/*",
      "!src/components/index.js",
      "!src/components/*/**/index.js",
      "!src/components/*/**/docDefinition.js",
      "!src/components/*/**/ChartCustomTooltip.js",
      "!src/store/*/sagas/index.js",
      "!src/store/*/schema.js",
      "!src/utils/fakeState/**/*",
      "!src/containers/*/index.js",
      "!src/store/*?/index.js",
      "!src/scenes/*?/index.js",
      "!src/scenes/*/**/index.js",
      "!src/constants.js"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
